<?xml version="1.0" encoding="utf-8"?>
<Settings>
    <!-- The unrealistic drive letter has been chosen 
    for tests that are expected to access a mock file system. 
    A test will throw an exception if it attempts to write to the real file system, 
    provided it is run on a computer that does not have that drive. -->
    <ProgramsFolder Path="J:\FalconProgrammer\Programs"/>
    <OriginalProgramsFolder Path="J:\FalconProgrammer\Original Programs"/>
    <TemplateProgramsFolder Path="J:\FalconProgrammer\Template Programs"/>
    <MustUseGuiScriptProcessor>
        <SoundBankCategory SoundBank="Falcon Factory" Category="Organic Texture 2.8"/>
        <SoundBankCategory SoundBank="Organic Keys" Category=""/>
        <SoundBankCategory SoundBank="Pulsar" Category=""/>
        <SoundBankCategory SoundBank="Voklm" Category=""/>
    </MustUseGuiScriptProcessor>
    <Backgrounds>
        <Background SoundBank="Eternal Funk" Path="J:\FalconProgrammer\Background Images\Yellowish Mid-Green.png"/>
        <Background SoundBank="Fluidity" Path="J:\FalconProgrammer\Background Images\Midnight Blue.png"/>
    </Backgrounds>
    <SoundBankSpecific>
        <Fluidity MoveAttackMacroToEnd="true"/>
        <OrganicPads AttackSeconds="0.02" ReleaseSeconds="0.3"
                     MaxAttackSeconds="1" MaxDecaySeconds="15" MaxReleaseSeconds="2"/>
        <StandardLayoutSoundBanks>
        </StandardLayoutSoundBanks>
    </SoundBankSpecific>
</Settings>