                <script><![CDATA[--------------------------------------------------------------------------------
-- Multidimensional Polyphonic Expression (MPE)
-- 
-- map x to pitch, z to gain by default (can be disabled)
-- and send x, y, z on ScriptEventModulations 2, 1, 0
--------------------------------------------------------------------------------

local X_To = Menu {"X", {"None", "Pitch", "Mod 2 (Bi)"}, selected = 2, displayName = "Pitch (X)"}
X_To.tooltip = "Mod source or parameter driven by the X axis. (Mod 2 is the Script Event Modulation with Event Id 2"
.. " where modulation is relative (use Bipolar))"
moveControl(X_To, 1, 1)
local PitchBendRange = NumBox {"PitchBendRange", 48, 12, 48, true, displayName = "PB Range", tooltip = "Pitch Bend Range"}
moveControl(PitchBendRange, 1, 3)
PitchBendRange.tooltip = "PitchBend range in Semitones. Usually 24 or 48"

local Y_To = Menu{"Y", {"None", "PolyAT", "Mod 1 abs (Uni)", "Mod 1 rel (Bi)"}, selected = 2, displayName = "Timbre (Y)"}
Y_To.tooltip = "Mod source driven by the Y axis. (Mod 1 is the Script Event Modulation with Event Id 1"
.. " where modulation is either absolute (use Unipolar) or relative (use Bipolar))"
moveControl(Y_To, 2, 1)

local Z_To = Menu{"Z", {"None", "Gain", "PolyAT", "Mod 0 abs (Uni)", "Mod 0 rel (Bi)"}, selected = 2, displayName = "Pressure (Z)"}
Z_To.tooltip = "Mod source or parameter driven by the Z axis. (Mod 0 is the Script Event Modulation with Event Id 0"
.. " where modulation is either absolute (use Unipolar) or relative (use Bipolar))"
moveControl(Z_To, 3, 1)
local InitialPressure = NumBox{"InitialPressure", 0, 0, 1, displayName="Init Pressure", tooltip = "Initial Pressure"}
InitialPressure.tooltip = "Initial value for Z axis when not yet sent by the controler"
moveControl(InitialPressure, 3, 3)

local GlobalChannel = NumBox { "GlobalChannel", 1, 1, 16, true, displayName = "Global Channel", tooltip = "Global MIDI Channel"}
GlobalChannel.tooltip = "MIDI Channel where are sent global commands in order to control all voices at once"
moveControl(GlobalChannel, 4, 2)
local GainMap = Menu{"GainMap", {"20dB", "z^2", "Linear"}, displayName = "Gain Map"}
GainMap.tooltip = "Automatic gain remapping when Z axis is used to control Gain"
moveControl(GainMap, 5, 1)

function onInit()
	Part:setParameter("MidiChannel", -1) -- put channel in omni mode
end

local ids = {}
local state = {}
for ch=1,16 do 
	state[ch] = {}
	state[ch].id = nil 
	state[ch].note = nil
	state[ch].initialBend = nil
	state[ch].initialTimbre = nil
	state[ch].initialpressure = nil
end

local pressureToGaindB = function (p)
	local dB = -12 + 20 * p
	local g = math.pow(10.0, dB / 20)
	return g
end

local pressureToGaindQuad = function (p)
	return p * p
end

local pressureToGainLinear = function (p)
	return p
end

local pressureToGainFunctions = {pressureToGaindB, pressureToGaindQuad, pressureToGainLinear}
local pressureToGain = function(p)
	return pressureToGainFunctions[GainMap.value](p)
end

function onNote(e)
	local ch = e.channel or 1 -- can happen if there is no channel like when sent from the UI
	local n = e.note
    local id = postEvent(e)
	
	state[ch].id = id
	state[ch].note = n
	
	ids[id] = {}
	ids[id].note = n

	local bend = state[ch].initialBend or 0
	local timbre = state[ch].initialTimbre or 0
	local pressure = state[ch].initialpressure or InitialPressure.value
	
	sendXValue(bend, id, ch)
	sendYValue(timbre, id, ch)
	sendZValue(pressure, id, ch, true)
end

function onRelease(e)
	local ch = e.channel or 1 -- can happen if there is no channel like when sent from the UI
	postEvent(e)
	if ch ~= GlobalChannel.value then
		if state[ch].id ~= nil then
			ids[state[ch].id] = nil
		end
		state[ch].id = nil
		state[ch].note = nil
		state[ch].initialTimbre = nil
		state[ch].initialPressure = nil
	end
end

function sendXValue(value, id, ch)
	if id then
		if X_To.value == 2 then		
			changeTune(id, value)
		elseif X_To.value == 3 then -- X to bipolar script modulation
			sendScriptModulation(2, value / PitchBendRange.value, 20, id)
		end
	end
end

function onPitchBend(e)
	local ch = e.channel or 1 -- can happen if there is no channel like when sent from the UI
	local bend = PitchBendRange.value * e.bend
	
	if ch == GlobalChannel.value then
		for i=1, 16 do
			if i ~= GlobalChannel.value then
				state[i].initialBend = bend
				sendXValue(bend, state[i].id, i)
			end
		end
	else
		state[ch].initialBend = bend
		sendXValue(bend, state[ch].id, ch)
	end
end

function sendYValue(value, id, ch)
	if id then
		if Y_To.value == 2 then -- Y to PolyAT
			polyAfterTouch(value* 127, ids[id].note)
		elseif Y_To.value == 3 then -- Y to unipolar script modulation
			sendScriptModulation(1, value, 20, id)
		elseif Y_To.value == 4 then -- Y to bipolar script modulation
			if state[ch].initialTimbre ~= nil then -- check that we can be relative to something
				local newValue = nil
				if value >= state[ch].initialTimbre then
					newValue = (value - state[ch].initialTimbre) / (1. - state[ch].initialTimbre)
				else
					newValue = -(state[ch].initialTimbre - value) / (state[ch].initialTimbre)
				end
				sendScriptModulation(1, newValue, 20, id)
			end
		end
	end
end

function onController(e) 
	local cc = e.controller
	if cc == 74 then
		local ch = e.channel or 1 -- can happen if there is no channel like when sent from the UI
		local value = e.value / 127.
				
		if ch == GlobalChannel.value then
			for i=1, 16 do
				if i ~= GlobalChannel.value then
					if state[i].initialTimbre == nil then
						state[i].initialTimbre = value
					end
					sendYValue(value, state[i].id, i)
				end
			end
		else
			if state[ch].initialTimbre == nil then
				state[ch].initialTimbre = value
			end
			sendYValue(value, state[ch].id, ch)
		end
	else
		postEvent(e)	
	end
end

function sendZValue(value, id, ch, immediate)
	if id then		
		if Z_To.value == 2 then -- Z to gain
			local g = pressureToGain(value)
			local rampTime = 20
			if immediate == true then
				rampTime = 0
			end
			fade(id, g, rampTime)
		elseif Z_To.value == 3 then -- Z to polyAT
			polyAfterTouch(value * 127, ids[id].note)
		elseif Z_To.value == 4 then -- Z to unipolar script modulation
			sendScriptModulation(0, value, 20, id)
		elseif Z_To.value == 5 then -- Z to bipolar script modulation
			if state[ch].initialPressure ~= nil then -- check that we can be relative to something
				local newValue = nil
				if value >= state[ch].initialPressure then
					newValue = (value - state[ch].initialPressure) / (1. - state[ch].initialPressure)
				else
					newValue = -(state[ch].initialPressure - value) / (state[ch].initialPressure)
				end
				sendScriptModulation(0, newValue, 20, id)
			end
		end
	end
end

function onAfterTouch(e)
	local ch = e.channel or 1 -- can happen if there is no channel like when sent from the UI
	local value = e.value / 127.
	
	if ch == GlobalChannel.value then
		for i=1, 16 do
			if i ~= GlobalChannel.value then
				if state[i].initialPressure == nil then
					state[i].initialPressure = value
				end
				sendZValue(value, state[i].id, ch, false)
			end
		end
	else
		if state[ch].initialPressure == nil then
			state[ch].initialPressure = value
		end
		sendZValue(value, state[ch].id, ch, false)
	end
end]]></script>
